            let basketLines = document.querySelectorAll('#basketLines li input');
            basketLines.forEach((element) => {
                let gtmJson = JSON.parse(this.getAttribute("data-gtm-click"));
                let theId = element.getAttribute("id");
                for (let index = 0; gtmJson.ecommerce.items.length; index++) {
                    if (gtmJson.ecommerce.items[index].item_id !== theId) {
                        continue;
                    }
                    gtmJson.ecommerce.items[index].quantity = element.value;
