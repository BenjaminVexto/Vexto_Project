llToActionText":"content.ctaText.null","secondaryCallToActionUseAnalytics":false,"secondaryOpenInNewTab":false,"__typename":"TextBlockChild"},"ContentContainer:cs3eec41ae27a8b74d":{"id":"cs3eec41ae27a8b74d","multivariate":null,"targeting":null,"__typename":"ContentContainer","section":{"type":"id","generated":false,"id":"CardContentSection:blt5b90a570ded134cd","typename":"CardContentSection"}},"CardContentSection:blt5b90a570ded134cd":{"__typename":"CardContentSection","id":"blt5b90a570ded134cd","layout":{"type":"id","generated":true,"id":"$CardContentSection:blt5b90a570ded134cd.layout","typename":"ContentSectionLayout"},"layoutLegacy":{"type":"id","generated":true,"id":"$CardContentSection:blt5b90a570ded134cd.layoutLegacy","typename":"ContentSectionLayoutLegacy"},"moduleTitle":"","showModuleTitle":false,"backgroundColor":"#FF9FF8","blocks":[{"type":"id","generated":true,"id":"CardContentSection:blt5b90a570ded134cd.blocks.0","typename":"CardContentChild"},{"type":"id","generated":true,"id":"CardContentSection:blt5b90a570ded134cd.blocks.1","typename":"CardContentChild"},{"type":"id","generated":true,"id":"CardContentSection:blt5b90a570ded134cd.blocks.2","typename":"CardContentChild"},{"type":"id","generated":true,"id":"CardContentSection:blt5b90a570ded134cd.blocks.3","typename":"CardContentChild"},{"type":"id","generated":true,"id":"CardContentSection:blt5b90a570ded134cd.blocks.4","typename":"CardContentChild"},{"type":"id","generated":true,"id":"CardContentSection:blt5b90a570ded134cd.blocks.5","typename":"CardContentChild"}],"mediaControls":{"type":"id","generated":true,"id":"$CardContentSection:blt5b90a570ded134cd.mediaControls","typename":"MediaControls"},"previousAriaLabel":"Forrige","nextAriaLabel":"Næste"},"$CardContentSection:blt5b90a570ded134cd.layout":{"width":"Standard","colors":{"type":"id","generated":true,"id":"$CardContentSection:blt5b90a570ded134cd.layout.colors","typename":"ContentSectionLayoutColors"},"__typename":"ContentSectionLayout"},"$CardContentSection:blt5b90a570ded134cd.layout.colors":{"background":"#FF9FF8","__typename":"ContentSectionLayoutColors"},"$CardContentSection:blt5b90a570ded134cd.layoutLegacy":{"fullWidth":false,"removePadding":false,"__typename":"ContentSectionLayoutLegacy"},"CardContentSection:blt5b90a570ded134cd.blocks.0":{"title":"Indsats frem for perfektion","isH1":false,"description":"Frygten for det uperfekte er den mest almindelige frygt blandt kreative skabere (det og så frygten for at mangle en klods!). Lær dine piger at lægge bekymringerne til side og bare fokusere på at få noget fra hånden – det behøver ikke at være perfekt, det skal bare være. Og det kan være lige det, hun vil.  ","textAlignment":"CENTER","primaryLogoSrc":null,"secondaryLogoSrc":null,"logoPosition":"right","imageSrc":{"type":"id","generated":true,"id":"$CardContentSection:blt5b90a570ded134cd.blocks.0.imageSrc","typename":"ImageAssetDetails"},"callToActionText":"content.ctaText.null","callToActionLink":"","callToActionUseAnalytics":false,"callToActionOpenInNewTab":false,"accountActionValue":null,"accountActionReturnUrl":"","altText":"","contrast":"Dark","videoMedia":null,"videoModalCloseButtonAriaLabel":"Luk","__typename":"CardContentChild"},"$CardContentSection:blt5b90a570ded134cd.blocks.0.imageSrc":{"url":"https://www.lego.com/cdn/cs/set/assets/blt39ca8bcaac93b6b5/07_Girls2HY25_CardContent_01.jpg?fit=crop\u0026format=jpg\u0026quality=80\u0026width=635\u0026height=440\u0026dpr=1","width":635,"height":440,"maxPixelDensity":1.5,"format":"jpg","__typename":"ImageAssetDetails"},"CardContentSection:blt5b90a570ded134cd.blocks.1":{"title":"Brug tilbageslag fornuftigt","isH1":false,"description":"En fejl? Det ord kender vi slet ikke. Det er, fordi der faktisk ikke findes noget, som hedder fejl, især når man leger. Der vil altid ske små uheld, og de hjælper os alle med at lære. Også os voksne. Dine piger vil elske at høre om dine egne udfordringer og lære, at tilbageslag slet ikke er noget negativt, hvis man har den rigtige indstilling.","textAlignment":"CENTER","primaryLogoSrc":null,"secondaryLogoSrc":null,"logoPosition":"right","imageSrc":{"type":"id","generated":true,"id":"$CardContentSection:blt5b90a570ded134cd.blocks.1.imageSrc","typename":"ImageAssetDetails"},"callToActionText":"content.ctaText.null","callToActionLink":"","callToActionUseAnalytics":false,"callToActionOpenInNewTab":false,"accountActionValue":null,"accountActionReturnUrl":"","altText":"","contrast":"Dark","videoMedia":null,"videoModalCloseButtonAriaLabel":"Luk","__typename":"CardContentChild"},"$CardContentSection:blt5b90a570ded134cd.blocks.1.imageSrc":{"url":"https://www.lego.com/cdn/cs/set/assets/blt1b121d61448ebea9/07_Girls2HY25_CardContent_02.jpg?fit=crop\u0026format=jpg\u0026quality=80\u0026width=635\u0026height=440\u0026dpr=1","width":635,"height":440,"maxPixelDensity":1.5,"format":"jpg","__typename":"ImageAssetDetails"},"CardContentSection:blt5b90a570ded134cd.blocks.2":{"title":"Opbyg tolerance over for frustration","isH1":false,"description":"Frustration er en vigtig del af den kreative proces, fordi den hjælper os med at overvinde forhindringer. Så sig til dine piger, at de kan bruge deres frustration konstruktivt ved at blive ved med at prøve og skabe.","textAlignment":"CENTER","primaryLogoSrc":null,"secondaryLogoSrc":null,"logoPosition":"right","imageSrc":{"type":"id","generated":true,"id":"$CardContentSection:blt5b90a570ded134cd.blocks.2.imageSrc","typename":"ImageAssetDetails"},"callToActionText":"content.ctaText.null","callToActionLink":"","callToActionUseAnalytics":false,"callToActionOpenInNewTab":false,"accountActionValue":null,"accountActionReturnUrl":"","altText":"","contrast":"Dark","videoMedia":null,"videoModalCloseButtonAriaLabel":"Luk","__typename":"CardContentChild"},"$CardContentSection:blt5b90a570ded134cd.blocks.2.imageSrc":{"url":"https://www.lego.com/cdn/cs/set/assets/bltc36104018b1d9e25/07_Girls2HY25_CardContent_03.jpg?fit=crop\u0026format=jpg\u0026quality=80\u0026width=635\u0026height=440\u0026dpr=1","width":635,"height":440,"maxPixelDensity":1.5,"format":"jpg","__typename":"ImageAssetDetails"},"CardContentSection:blt5b90a570ded134cd.blocks.3":{"title":"Opbyg tålmodighed for at vokse","isH1":false,"description":"Visse små ord rummer store perspektiver – f.eks. ordet \"endnu\".  Det er ikke altid nemt at prøve noget nyt. Det kræver engagement og øvelse at blive god til noget. Nogle gange er det bare et lille ord i slutningen af en sætning, der gør forskellen, f.eks. \"Det kan jeg ikke ... ENDNU\" eller \"Det er jeg ikke god til ... ENDNU\".","textAlignment":"CENTER","primaryLogoSrc":null,"secondaryLogoSrc":null,"logoPosition":"right","imageSrc":{"type":"id","generated":true,"id":"$CardContentSection:blt5b90a570ded134cd.blocks.3.imageSrc","typename":"ImageAssetDetails"},"callToActionText":"content.ctaText.null","callToActionLink":"","callToActionUseAnalytics":false,"callToActionOpenInNewTab":false,"accountActionValue":null,"accountActionReturnUrl":"","altText":"","contrast":"Dark","videoMedia":null,"videoModalCloseButtonAriaLabel":"Luk","__typename":"CardContentChild"},"$CardContentSection:blt5b90a570ded134cd.blocks.3.imageSrc":{"url":"https://www.lego.com/cdn/cs/set/assets/blt6f1c1455a1031ddb/07_Girls2HY25_CardContent_04.jpg?fit=crop\u0026format=jpg\u0026quality=80\u0026width=635\u0026height=440\u0026dpr=1","width":635,"height":440,"maxPixelDensity":1.5,"format":"jpg","__typename":"ImageAssetDetails"},"CardContentSection:blt5b90a570ded134cd.blocks.4":{"title":"Giv anderledes ros","isH1":false,"description":"Voksne fokuserer ofte på at rose resultatet eller præstationen frem for indsatsen eller processen. Ved at rose idéen, nysgerrigheden eller indsatsen inspirerer du piger til at prøve nye ting og holde gang i kreativiteten.","textAlignment":"CENTER","primaryLogoSrc":null,"secondaryLogoSrc":null,"logoPosition":"right","imageSrc":{"type":"id","generated":true,"id":"$CardContentSection:blt5b90a570ded134cd.blocks.4.imageSrc","typename":"ImageAssetDetails"},"callToActionText":"content.ctaText.null","callToActionLink":"","callToActionUseAnalytics":false,"callToActionOpenInNewTab":false,"accountActionValue":null,"accountActionReturnUrl":"","altText":"","contrast":"Dark","videoMedia":null,"videoModalCloseButtonAriaLabel":"Luk","__typename":"CardContentChild"},"$CardContentSection:blt5b90a570ded134cd.blocks.4.imageSrc":{"url":"https://www.lego.com/cdn/cs/set/assets/blt947642fd6df681ad/07_Girls2HY25_CardContent_05.jpg?fit=crop\u0026format=jpg\u0026quality=80\u0026width=635\u0026height=440\u0026dpr=1","width":635,"height":440,"maxPixelDensity":1.5,"format":"jpg","__typename":"ImageAssetDetails"},"CardContentSection:blt5b90a570ded134cd.blocks.5":{"title":"Du kan også lege med!","isH1":false,"description":"Det er vigtigt for børn at have nogen at se op til og spejle sig i – det er sådan, de lærer. Og findes der nogen bedre rollemodel end dig selv? Ja, det er faktisk en åben invitation til (og en fremragende undskyldning for) at lægge arbejdet til side og begynde at lege!","textAlignment":"CENTER","primaryLogoSrc":null,"secondaryLogoSrc":null,"logoPosition":"right","imageSrc":{"type":"id","generated":true,"id":"$CardContentSection:blt5b90a570ded134cd.blocks.5.imageSrc","typename":"ImageAssetDetails"},"callToActionText":"content.ctaText.null","callToActionLink":"","callToActionUseAnalytics":false,"callToActionOpenInNewTab":false,"accountActionValue":null,"accountActionReturnUrl":"","altText":"","contrast":"Dark","videoMedia":null,"videoModalCloseButtonAriaLabel":"Luk","__typename":"CardContentChild"},"$CardContentSection:blt5b90a570ded134cd.blocks.5.imageSrc":{"url":"https://www.lego.com/cdn/cs/set/assets/bltdcbedbff969056c2/07_Girls2HY25_CardContent_05.jpg?fit=crop\u0026format=jpg\u0026quality=80\u0026width=635\u0026height=440\u0026dpr=1","width":635,"height":440,"maxPixelDensity":1.5,"format":"jpg","__typename":"ImageAssetDetails"},"$CardContentSection:blt5b90a570ded134cd.mediaControls":{"controlsGroup":"media.controls