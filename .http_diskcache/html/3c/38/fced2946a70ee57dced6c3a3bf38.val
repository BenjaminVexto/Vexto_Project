    let btns = document.querySelectorAll('button[data-gtm-item-id]');
    btns.forEach(function (btn) {
        btn.addEventListener('click', function () {
            let gtmId = this.getAttribute('data-gtm-item-id');
            getElements().forEach(el => {
                var gtmJson = JSON.parse(atob(el.dataset.gtm));
                for (var i = 0; i <= gtmJson.ecommerce.items.length; i++) {
                    if (gtmJson.ecommerce.items[i].item_id !== gtmId) {
                        continue;
                    }
                    window.dataLayer.push({ event: 'add_to_cart', ecommerce: { items: gtmJson.ecommerce.items[i] } });
                    return;
                }
            });
        },false);
    });
    let basketBuyBtn = document.querySelector('#CommerceBasketApp a.site-btn-green');
    if (basketBuyBtn) {
        basketBuyBtn.addEventListener('click', function (e) {
            let basketLines = document.querySelectorAll('#basketLines li input');
            basketLines.forEach((element) => {
                let gtmJson = JSON.parse(this.getAttribute("data-gtm-click"));
                let theId = element.getAttribute("id");
                for (let index = 0; gtmJson.ecommerce.items.length; index++) {
                    if (gtmJson.ecommerce.items[index].item_id !== theId) {
                        continue;
                    }
                    gtmJson.ecommerce.items[index].quantity = element.value;
                    window.dataLayer.push({ ecommerce: null })
                    window.dataLayer.push(gtmJson);
                    break;
                }
            });
        });
    }
    let paymentTypeButtons = document.getElementsByName("paymentOption");    
