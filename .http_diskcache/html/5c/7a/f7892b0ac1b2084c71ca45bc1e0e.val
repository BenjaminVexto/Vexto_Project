    <script type="text/javascript">
    document.addEventListener('DOMContentLoaded', () => {
        // Define target node and observer configuration
        const targetNode = document.getElementById("ccs-inline-content");
        const config = { childList: true, characterData: true, subtree: true, attributes: true };

        // Mutation Observer callback
        const callback = (mutationList, observer) => {
            for (const mutation of mutationList) {
                for (const node of mutation.addedNodes) {
                    if (node.nodeName === "SCRIPT") continue;

                    const showMoreContainer = document.getElementById("scrollToDescriptionContainer");
                    const descContainer = document.getElementById("description");

                    if (showMoreContainer && descContainer) {
                        showMoreContainer.classList.remove("not-visible");
                        descContainer.classList.remove("hidden");
                    }

                    observer.disconnect(); // Stop observing after first match
                    return; // Exit loop early
                }
            }
        };

        // Create and start the observer
        const observer = new MutationObserver(callback);
        if (targetNode) observer.observe(targetNode, config);

        // Toggle button functionality
        document.querySelectorAll('.site-toggleAccessoriesBtn').forEach((button) => {
            button.addEventListener('click', () => {
                const isExpanded = button.getAttribute('aria-expanded') === 'true';
                button.setAttribute('aria-expanded', !isExpanded);
                button.textContent = isExpanded
                    ? 'Vis flere'
                    : 'Vis færre';
            });
        });

        // Add class to body
        document.body.classList.add('product-details-page');

        // Main click handler
        document.addEventListener('click', ({ target }) => {
            if (target.matches('#scrollToDescription')) {
                scrollToDescription();
            } else if (target.classList.contains('accordionHeader')) {
                toggleAccordion(target);
            }
        });

        // Accordion toggle function
        function toggleAccordion(clickedElement) {
            const controlsId = clickedElement.getAttribute('aria-controls');
            if (!controlsId) return;

            const accordion = document.getElementById(controlsId);
            if (!accordion) return;

            const isExpanded = clickedElement.getAttribute('aria-expanded') === 'true';
            updateAccordion(!isExpanded, accordion, clickedElement);

            if (!isExpanded) accordion.focus();
        }

        // Scroll to description function
        function scrollToDescription() {
            const headerDesc = document.getElementById('accordionHeader_description');
            const panelDesc = document.getElementById('accordionPanel_description');

            if (!headerDesc || !panelDesc) return;

            updateAccordion(true, panelDesc, headerDesc);

            // Smooth scrolling using scrollIntoView
            headerDesc.scrollIntoView({ behavior: 'smooth', block: 'start' });
        }

        // Update accordion state function
        function updateAccordion(expand, panel, header) {
            const method = expand ? "add" : "remove";
            panel.classList[method]("active");
            header.parentNode.classList[method]("active");
            header.setAttribute("aria-expanded", expand.toString());
        }

        // Check if element is out of viewport
        function isElementOutOfViewport(element) {
            if (!element) return false;

            const rect = element.getBoundingClientRect();
            return rect.bottom < 0 || rect.top > window.innerHeight;
        }

        // Check button visibility and update class on body
        function checkButtonVisibility() {
            const button = document.querySelector('.site-new-product-page-design #addToCart_BtnForm .btn');

            if (isElementOutOfViewport(button)) {
                document.body.classList.add('bottom-ribbon-added');
            } else {
                document.body.classList.remove('bottom-ribbon-added');
            }
        }

        // Initial check and scroll listener for visibility updates
        checkButtonVisibility();
        window.addEventListener('scroll', checkButtonVisibility);
    });
 </script>

    
